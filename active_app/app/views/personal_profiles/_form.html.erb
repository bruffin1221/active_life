<%= form_for @profile do |f| %>
    <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
    </p>

    <p>
    <%= f.label :age %><br>
    <%= f.text_field :age %>
    </p>

    <p>
    <%= f.label :occupation %><br>
    <%= f.text_field :occupation %>
    </p>

    <p>
    <%= f.label :weight %><br>
    <%= f.text_field :weight %>
    <p>

    <p>
    <%= f.label :height %><br>
    <%= f.text_field :height %>
    </p>

    <p>
    <%= f.submit %>

    <% end %>


